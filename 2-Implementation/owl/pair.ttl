###  http://virtual-assembly.org/ontologies/pair#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :Agent ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasStatus ;
                            owl:someValuesFrom :ActivityStatus
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPart ;
                            owl:allValuesFrom :Activity
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSubjectType ;
                            owl:allValuesFrom :ActivityType
                          ] ;
          rdfs:comment "Activité"@en ,
                       """Une activité est entendue au sens très générique comme toute action entreprise par un Acteur avec une certaine finalité, à un certain moment, en un certain lieu.
Ce n'est _PAS_ une catégorie socio-économique au sens \"La ferme de la petite bourdaisière a pour activité Permaculture\"."""@fr ;
          rdfs:label "Activity"@en ,
                     "Activité"@fr .


###  http://virtual-assembly.org/ontologies/pair#ActivityField
:ActivityField rdf:type owl:Class ;
               rdfs:subClassOf :Concept ;
               rdfs:comment "A high-level class of sectors, fields and branches of activity."@en ,
                            "Classe de haut niveau rassemblant les secteurs, domaines et branches d'activité."@fr ;
               rdfs:label "Activity field"@en ,
                          "Champ d'activité"@fr .


